<p id="notice"><%= notice %></p>

<h1>Proffesions</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @proffesions.each do |proffesion| %>
      <tr>
        <td><%= proffesion.name %></td>
        <td><%= link_to 'Show', proffesion %></td>
        <td><%= link_to 'Edit', edit_proffesion_path(proffesion) %></td>
        <td><%= link_to 'Destroy', proffesion, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Proffesion', new_proffesion_path %>
